<launch>

	<arg name="paused" default="false"/>

	<arg name="use_sim_time" default="true"/>

	<arg name="gui" default="true"/>

	<arg name="headless" default="false"/>

	<arg name="debug" default="false"/>

	<arg name="x_pos" default="0.6"/>

  	<arg name="y_pos" default="3.2"/>

  	<arg name="z_pos" default="0.12"/>
	
	<arg name="yo_pos" default="0"/>

	<!--include file="$(find gazebo_ros)/launch/empty_world.launch">

		<arg name="debug" value="$(arg debug)"/>

		<arg name="gui" value="$(arg gui)"/>

		<arg name="paused" value="$(arg paused)"/>

		<arg name="use_sim_time" value="$(arg use_sim_time)"/>

		<arg name="headless" value="$(arg headless)"/>

	</include-->



	<param name="robot_description" command="$(find xacro)/xacro --inorder '/home/jyj/catkin_ws/src/p31/urdf/p31.urdf'"/>

	

	<node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen" args="-urdf -model myrobot -x $(arg x_pos) -y $(arg y_pos) -z $(arg z_pos) -Y $(arg yo_pos) -param robot_description"/>
</launch>